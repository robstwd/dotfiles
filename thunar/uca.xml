	<description>Copies file&apos;s full pathname to the X Primary selection</description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon></icon>
	<name>Compare</name>
	<unique-id>1417289736054708-3</unique-id>
